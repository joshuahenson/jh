{
  "extends": "airbnb-base",
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "func-names": 0,
    "import/newline-after-import": 0,
    "max-len": 0,
    "no-console": 0,
    "no-mixed-operators": 0,
    "no-plusplus": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0
  }
}
